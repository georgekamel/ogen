{{- /*gotype: github.com/ogen-go/ogen/gen/ir.Type*/ -}}
{{- define "schema/struct" }}
{{- if eq (len $.Fields) 0 }}{{/* Blank struct */}}
	type {{ $.Name }} struct{}
{{- else }}
	type {{ $.Name }} struct {
	{{- range $f := $.Fields }}
		{{- template "godoc" $f.GoDoc }}
		{{- $const := $f.Const }}
		{{- if $const.Set }}
			{{- $t := $f.Type }}
			{{- $j := $t.JSON }}
			{{- $val := print_go $const.Value }}
			{{- if $j.TimeFormat }}
				{{ $f.Name }} {{ $t.Go }} = func() {{ $t.Go }} { v, _ := json.DecodeTimeFormat(jx.DecodeStr({{ quote $val }}), {{ $j.TimeFormat }}); return v }()
			{{- else if $j.Decoder }}
				{{ $f.Name }} {{ $t.Go }} = func() {{ $t.Go }} { v, _ := json.Decode{{ $j.Decoder }}(jx.DecodeStr({{ quote $val }})); return v }()
			{{- else if $j.IsBase64 }}
				{{ $f.Name }} {{ $t.Go }} = func() {{ $t.Go }} { v, _ := jx.DecodeStr({{ quote $val }}).Base64(); return v }()
			{{- else }}
				{{ $f.Name }} {{ $t.Go }} = {{ $t.Go }}({{ $val }})
			{{- end }} {{ if $f.Tag.GetTags }}{{ backquote $f.Tag.GetTags }}{{ end }}
		{{- else }}
			{{ $f.Name }} {{ $f.Type.Go }} {{ if $f.Tag.GetTags }}{{ backquote $f.Tag.GetTags }}{{ end }}
		{{- end }}
	{{- end }}
	}
{{- end }}

{{- range $f := $.Fields }}
// Get{{ $f.Name }} returns the value of {{ $f.Name }}.
func (s {{ $.ReadOnlyReceiver }}) Get{{ $f.Name }}() {{ $f.Type.Go }} {
	return s.{{ $f.Name }}
}
{{- end }}
{{- range $f := $.Fields }}
// Set{{ $f.Name }} sets the value of {{ $f.Name }}.
func (s *{{ $.Name }}) Set{{ $f.Name }}(val {{ $f.Type.Go }}) {
	s.{{ $f.Name }} = val
}
{{- end }}

{{ end }}
